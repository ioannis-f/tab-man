{
  "manifest_version": 2,
  "name": "Tab-man",
  "version": "0.8.0",
  "description": "Tab manager ",
//  "homepage_url": "",
  "permissions": [
    "activeTab",
//    "clipboardRead",
//    "clipboardWrite",
//    "clipboard-read",
//    "clipboard-write",
    "contextMenus",
    "notifications",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icons/tab-man5.png",
    "browser_style": true,
    "default_title": "Tab-man",
    "default_popup": "popup/popup.html"
  },
    "background": {
    "scripts": [
      "background.js"
    ]
  },

  "icons": {
//    "48": "icons/icon.svg",
    "96": "icons/tab-man5.png"
  }
}
